{
    "name": "friendly-snippets",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "plaintext",
                    "markdown",
                    "tex",
                    "html",
                    "global",
                    "all"
                ],
                "path": "./snippets/global.json"
            },
            {
                "language": "cpp",
                "path": "./snippets/cpp/cpp.json"
            },
            {
                "language": "python",
                "path": "./snippets/python/python.json"
            },
                        {
                "language": [
                    "html"
                ],
                "path": "./snippets/html.json"
            }
        ]
    }
}
